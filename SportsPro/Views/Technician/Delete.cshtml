@model Technician
@{
	ViewBag.Title = "Delete Technician";
}
<h2 class="mb-3">Delete Technician</h2>
<form asp-action="Delete">
	<input type="hidden" asp-for="TechnicianID" />
	<p>Are you sure you want to delete: <strong>@Model.Name</strong></p>
	<button type="submit" class="btn btn-danger">Yes</button>
	<a asp-action="List" class="btn btn-secondary">No</a>
</form>

